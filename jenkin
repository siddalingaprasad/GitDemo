pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'echo "this is build stage" '
            }
        }
        stage('Test') {
            steps {
               sh 'echo "this is testing stage" '
            }
        }
        stage('Deploy') {
            steps {
               sh 'echo "this is build stage" '
            }
        }
    }
}
